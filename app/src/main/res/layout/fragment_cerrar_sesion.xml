<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".uiCerrarSesion.CerrarSesionFragment">

    <!-- ImageView centrado vertical y horizontalmente -->

    <!-- TextView centrado vertical y horizontalmente y colocado debajo de la ImageView -->

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="248dp"
        android:layout_height="201dp"
        android:layout_marginStart="81dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="82dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:srcCompat="@drawable/logoapp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Gracias por Utilizar Voluntarios Conectados"
        android:textAlignment="center"
        android:textSize="25sp"
        android:textStyle="bold" />

</FrameLayout>
